{
  "mappings": [
    {
      "pattern": "\\b(codex|cdx)\\b",
      "snippet": "codex.md"
    },
    {
      "pattern": "\\b(email|mail|e-mail|message|inbox|send\\s+(to|message))\\b",
      "snippet": "mail.md"
    },
    {
      "pattern": "\\b(gcal|g-cal|google\\s*calendar|calendar|event|schedule|appointment)\\b",
      "snippet": "gcal.md"
    },
    {
      "pattern": "\\bHTML\\b",
      "snippet": "HTML.md"
    },
    {
      "pattern": "\\bSTYLE\\b",
      "snippet": "style.md"
    }
  ]
}